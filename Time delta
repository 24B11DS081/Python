import math
import os
import random
import re
import sys
from datetime import datetime

def time_delta(t1, t2):
    # The format string based on: Day dd Mon yyyy hh:mm:ss +xxxx
    fmt = '%a %d %b %Y %H:%M:%S %z'
    
    # Parse the two timestamp strings into datetime objects
    dt1 = datetime.strptime(t1, fmt)
    dt2 = datetime.strptime(t2, fmt)
    
    # Calculate absolute difference in seconds
    # total_seconds() returns a float, so we cast to int as per output format
    diff = int(abs((dt1 - dt2).total_seconds()))
    
    return str(diff)

if __name__ == '__main__':

    t = int(input())

    for t_itr in range(t):
        t1 = input()
        t2 = input()

        delta = time_delta(t1, t2)
        print(delta)
